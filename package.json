{
  "name": "datahawk-autonomous-qa",
  "version": "1.2.0",
  "description": "Autonomous QA Browser Agent for CSV validation with web interfaces",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/main.ts",
    "start": "node dist/main.js",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "validate": "npm run lint && npm run test:unit",
    "crew:install": "pip install -r requirements.txt",
    "models:download": "node scripts/download-models.js"
  },
  "keywords": [
    "automation",
    "qa",
    "csv-validation",
    "browser-automation",
    "playwright",
    "ocr",
    "llm",
    "crewai"
  ],
  "author": "Diego Nogueira",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.8.0"
  },
  "dependencies": {
    "@playwright/test": "^1.44.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "date-fns": "^3.0.6",
    "dotenv": "^16.4.5",
    "jimp": "^0.22.10",
    "lodash": "^4.17.21",
    "papaparse": "^5.4.1",
    "playwright": "^1.54.1",
    "progress": "^2.0.3",
    "sharp": "^0.33.2",
    "tesseract.js": "^5.0.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "yaml": "^2.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.0",
    "@types/papaparse": "^5.3.14",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "warn",
      "prefer-const": "error",
      "no-console": "warn"
    }
  }
}
